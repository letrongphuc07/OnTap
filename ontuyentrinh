#include<stdio.h>
int tuyentinh(int a[],int n,int x){
	for(int i=0;i<n;i++)
	if(a[i]==x)
	return i;
	return -1;
}
int main(){
	int a[]={2,3,4,10,40};
	int x=10;
	int n=sizeof(a)/sizeof(a[0]);
	int kq= tuyentinh(a,n,x);
  if (kq != -1)
    printf("%d xuat hien tai chi so %d", x, kq);
  else
    printf("%d xuat hien tai chi so %d", x, kq);
  return 0;
}
	
