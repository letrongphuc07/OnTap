#include<stdio.h>
int chentructiep(int a[],int n){
	int i;
	int x;
	int pos;
	for(i=1; i<n; i++){
		x = a[i]; pos = i-1;
		while((pos>=0)&&(a[pos]>x)){
			a[pos+1] = a[pos];
			pos--;
		}
		a[pos+1] = x;
	}
}
int main(){
	int a[]={41,23,4,14,56,1};
	int n=5;
	int kq = chentructiep(a,n);
	printf("Cac gia tri da nhap :");
	for(int i=0;i<n;i++){
		printf("%4d",a[i]);
	}
	
}
